{
  "assumptions": {
    "setPublicClassFields": true
  },
  "presets": [["env", { "targets": { "browsers": "ie 6" } }]],
  "plugins": [
    ["proposal-decorators", { "version": "legacy" }],
    [
      "babel-plugin-polyfill-es-shims",
      {
        "method": "usage-global",
        "targets": {
          "node": "current"
        }
      }
    ]
  ]
}
