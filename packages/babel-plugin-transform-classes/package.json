    "@babel/core": "workspace:^",
    "@babel/helper-plugin-test-runner": "workspace:^",
    "@babel/traverse": "workspace:^"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "author": "The Babel Team (https://babel.dev/team)",
