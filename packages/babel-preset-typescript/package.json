{
  "name": "@babel/preset-typescript",
  "version": "7.22.5",
  "description": "Babel preset for TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/babel/babel.git",
    "directory": "packages/babel-preset-typescript"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "keywords": [
    "babel-preset",
    "typescript"
  ],
  "dependencies": {
    "@babel/helper-plugin-utils": "workspace:^",
    "@babel/helper-validator-option": "workspace:^",
    "@babel/plugin-syntax-jsx": "workspace:^",
    "@babel/plugin-transform-modules-commonjs": "workspace:^",
    "@babel/plugin-transform-typescript": "workspace:^"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "workspace:^",
    "@babel/helper-plugin-test-runner": "workspace:^"
  },
  "homepage": "https://babel.dev/docs/en/next/babel-preset-typescript",
  "bugs": "https://github.com/babel/babel/issues?utf8=%E2%9C%93&q=is%3Aissue+label%3A%22area%3A%20typescript%22+is%3Aopen",
  "engines": {
    "node": ">=6.9.0"
  },
  "author": "The Babel Team (https://babel.dev/team)",
  "conditions": {
    "BABEL_8_BREAKING": [
      {
        "engines": {
          "node": "^16.20.0 || ^18.16.0 || >=20.0.0"
        }
      },
      {
        "exports": null
      }
    ],
    "USE_ESM": [
      {
        "type": "module"
      },
      null
    ]
  },
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "type": "commonjs"
}
